@model PublisherListViewModel

    <div class="col-md-9">
        <div class="row">

            @foreach (var publisher in Model.Publishers)
            {
                <div class="col-md-4">
                    @await Html.PartialAsync("_publisher",publisher)

                </div>
            }
        </div>
    </div>
</div>

